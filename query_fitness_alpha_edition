pemikiran pertama..

CREATE TABLE ASSESMENT(
	ID VARCHAR(50) NOT NULL,
	DATE DATE,
	TIME DATETIME,
	TRAINER VARCHAR(50),
PRIMARY KEY (ID)
)ENGINE=INNODB,CHARSET=UTF8;

CREATE TABLE BODYFATANALISYSSKINFOLD_SCORE(
	ID VARCHAR(50) NOT NULL,
	ID_ASSESMENT VARCHAR(50)
	BICEPS INT(50),
	TRICEPS INT(50),
	SUBCAPULAR INT(50),
	SUPRAILIAC INT(50),
	TOTAL INT(500),
	PRECENTAGEFAT INT(500)
PRIMARY KEY (ID),
FOREIGN KEY (ID_ASSESMENT) REFERENCE ASSESMENT(ID)
)ENGINE=INNODB,CHARSET=UTF8;

CREATE TABLE BODYFATANALISYSSKINFOLD_RATE(
	ID VARCHAR(50) NOT NULL,
	ID_ASSESMENT VARCHAR(50)
	BICEPS INT(50),
	TRICEPS INT(50),
	SUBCAPULAR INT(50),
	SUPRAILIAC INT(50),
	TOTAL INT(500),
	PRECENTAGEFAT INT(500)
PRIMARY KEY (ID),
FOREIGN KEY (ID_ASSESMENT) REFERENCE ASSESMENT(ID)
)ENGINE=INNODB,CHARSET=UTF8;

CREATE TABLE BODYFATANALISYSMEASUREMENT_SCORE(
	ID VARCHAR(50) NOT NULL,
	ID_ASSESMENT VARCHAR(50)
	WAIST INT(50),
	HIPS INT(50),
	HIPS INT(50),
	HEARTRATE INT(50),
	BLOODPRESSURE VARCHAR(50)
PRIMARY KEY (ID),
FOREIGN KEY (ID_ASSESMENT) REFERENCE ASSESMENT(ID)
)ENGINE=INNODB,CHARSET=UTF8;

CREATE TABLE BODYFATANALISYSMEASUREMENT_RATE(
	ID VARCHAR(50) NOT NULL,
	ID_ASSESMENT VARCHAR(50)
	WAIST INT(50),
	HIPS INT(50),
	HIPS INT(50),
	HEARTRATE INT(50),
	BLOODPRESSURE VARCHAR(50)
PRIMARY KEY (ID),
FOREIGN KEY (ID_ASSESMENT) REFERENCE ASSESMENT(ID)
)ENGINE=INNODB,CHARSET=UTF8;


CREATE TABLE CARDIOFITNESSTEST_SCORE(
	ID VARCHAR(50) NOT NULL,
	ID_ASSESMENT VARCHAR(50)
	TREADMILL INT(50),
PRIMARY KEY (ID),
FOREIGN KEY (ID_ASSESMENT) REFERENCE ASSESMENT(ID)
)ENGINE=INNODB,CHARSET=UTF8;

CREATE TABLE CARDIOFITNESSTEST_RATE(
	ID VARCHAR(50) NOT NULL,
	ID_ASSESMENT VARCHAR(50)
	TREADMILL INT(50),
PRIMARY KEY (ID),
FOREIGN KEY (ID_ASSESMENT) REFERENCE ASSESMENT(ID)
)ENGINE=INNODB,CHARSET=UTF8;

