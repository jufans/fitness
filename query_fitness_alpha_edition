INSERT INTO `NAMA_DATABASE`.`TYPE_LATIHAN` (`ID`,`TYPE_ANALYSIS`) VALUES ('12', 'TREADMILL');

CREATE TABLE BODY_FAT_ANALYSIS (
	ID VARCHAR(50) NOT NULL,
	ID_ASSESMENT VARCHAR(50) NOT NULL,
	ID_TYPE_ANALYSIS VARCHAR(50),
	SCORE INT(50),
	RATING INT(50),
PRIMARY KEY (ID),
FOREIGN KEY (ID_ASSESMENT) REFERENCES ASSESMENT(ID),
FOREIGN KEY (ID_TYPE_ANALYSIS) REFERENCES TYPE_LATIHAN(ID)
)ENGINE=INNODB,CHARSET=UTF8;


