pemikiran kedua.. 


CREATE TABLE ASSESMENT(
	ID VARCHAR(50) NOT NULL,
	DATE DATE,
	TIME DATETIME,
	TRAINER VARCHAR(50),
PRIMARY KEY (ID)
)ENGINE=INNODB,CHARSET=UTF8;

CREATE TABLE TYPE_LATIHAN(
	ID VARCHAR(50) NOT NULL,
	TYPE_ANALYSIS VARCHAR(50) NOT NULL,
PRIMARY KEY (ID)
)ENGINE=INNODB,CHARSET=UTF8;

INSERT INTO `NAMA_DATABASE`.`TYPE_LATIHAN` (`ID`,`TYPE_ANALYSIS`) VALUES ('1', 'BICEPS SKINFOLD');
